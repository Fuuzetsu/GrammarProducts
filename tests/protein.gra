Grammar: DNA

NT: F{3}
T:  c

F{i} -> stay <<< F{i}   c c c
F{i} -> orf1 <<< F{i+1} c c
F{i} -> orf2 <<< F{i+2} c
F{i} -> del  <<< F{i}

//



Grammar: DNAdone

NT: F{3}
T:  empty

F{i} -> nil <<< empty

//



Grammar: DNAstand

NT: F{3}

F{i} -> del <<< F{i}

//



Grammar: Protein

NT: P
T:  a
T:  empty

P -> amino <<< P a
P -> del   <<< P
P -> nil   <<< empty

//



Product: DNAProtein

DNA >< Protein

//

